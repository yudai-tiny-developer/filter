{
	"manifest_version": 3,
	"name": "Filter for YouTube",
	"version": "1.3.0",
	"description": "Filter for YouTube",
	"default_locale": "en",
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"content_scripts": [
		{
			"js": [
				"content.js"
			],
			"css": [
				"content.css"
			],
			"run_at": "document_end",
			"matches": [
				"*://www.youtube.com/*"
			]
		}
	],
	"web_accessible_resources": [
		{
			"resources": [
				"lang/*.js"
			],
			"matches": [
				"*://www.youtube.com/*"
			]
		}
	]
}